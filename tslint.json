{
  "rules": {
    "max-line-length": [
      true,
      {"limit": 80, "ignore-pattern": "^import |^export {(.*?)} |@see"}
    ],
    "no-debugger": true,
    "no-duplicate-imports": true,
    "no-duplicate-variable": true,
    "no-trailing-whitespace": true,
    "no-var-keyword": true,
    "variable-name": [true, "ban-keywords"]
  }
}
